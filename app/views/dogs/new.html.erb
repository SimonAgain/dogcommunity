<h2> Create dog profile</h2>

<%= form_for @dog do |f| %>

  <%= f.label :name %> <br>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :breed %> <br>
  <%= f.text_field :breed %>
  <br><br>

  <%= f.label :bio, 'About:' %> <br>
  <%= f.text_field :bio %>
  <br><br>

  <%= f.label :image, 'Profile Image' %> <br>
  <%= f.text_field :image %>
  <br><br>

  <%= f.submit 'Submit Edit'  %>

<% end %>
